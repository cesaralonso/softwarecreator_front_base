{
  "name": "softwarecreator",
  "version": "1.0.2",
  "description": "Software Creator",
  "author": "Software Insights",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "rimraf": "rimraf",
    "changelog": "standard-changelog",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "start:prod": "ng serve --configuration production",
    "start:prod:aot": "ng serve --configuration production --aot",
    "build": "npm run clean:dist && ng build",
    "build:prod": "npm run build -- --configuration production",
    "test": "ng test -sr",
    "test:coverage": "npm run clean:coverage && ng test -sr -cc",
    "lint": "ng lint",
    "lint:styles": "stylelint ./src/app/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "e2e": "ng e2e",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "clean:coverage": "npm run rimraf -- coverage",
    "pwa-add": "ng add @angular/pwa",
    "sw-build": "ng build --configuration production && node build/fix-sw",
    "sw-serve": "npm run sw-build && http-server dist -p 8080 -o",
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "ci-deploy-fix": "ng build --configuration production --base-href \"https://calidad.acopiosroquin.com/\" --output-hashing none && npm run fix-memory-limit && node fix-sw",
    "ci-deploy": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration production --base-href \"https://calidad.acopiosroquin.com/\" --output-hashing none && node fix-sw",
    "ci-deploy-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration production --base-href \"https://calidad.acopiosroquin.com/\" && node fix-sw",
    "ci-deploy-local": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration production --base-href \"/\""
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0",
    "@angular/animations": "^13.2.2",
    "@angular/cdk": "~13.2.2",
    "@angular/cli": "^13.2.3",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/compiler-cli": "13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/platform-server": "13.2.2",
    "@angular/router": "13.2.2",
    "@angular/service-worker": "^13.2.2",
    "@auth0/angular-jwt": "^2.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "animate.css": "3.5.2",
    "bootstrap": "^5.1.3",
    "chart.js": "^2.9.3",
    "chroma-js": "1.3.3",
    "core-js": "2.4.1",
    "dexie": "3.0.1",
    "font-awesome": "4.7.0",
    "google-maps": "3.2.1",
    "ionicons": "2.0.1",
    "jquery": "^3.6.0",
    "jquery-slimscroll": "1.3.8",
    "js-file-download": "^0.4.12",
    "lodash": "4.17.4",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.32",
    "ngx-bootstrap": "^5.6.0",
    "ngx-mask": "^9.1.2",
    "normalize.css": "6.0.0",
    "rimraf": "^2.6.1",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "socket.io": "~2.2.0",
    "socket.io-client": "~2.2.0",
    "sockjs-client": "^1.1.4",
    "tether": "^1.4.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-devkit/schematics": "^13.2.3",
    "@types/bootstrap": "^5.0.0",
    "@types/fullcalendar": "2.7.40",
    "@types/googlemaps": "^3.39.8",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "2.0.41",
    "@types/jquery.slimscroll": "1.3.30",
    "@types/lodash": "^4.14.123",
    "@types/node": "^12.11.1",
