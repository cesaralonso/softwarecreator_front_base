    "stylelint": "7.10.1",
    "ts-node": "2.1.2",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "tslint-eslint-rules": "4.0.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^4.1.6",
